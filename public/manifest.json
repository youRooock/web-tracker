{
  "manifest_version": 2,

  "name": "Time Tracker",
  "description": "Track your time spent in web broser",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Time Tracker"
  },
  "icons": {
    "16": "logo48.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "background": {
    "page": "./background.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-nu4Ce4+yYyVoFT162LafGkde0OAEkxJgof9C8VBW53A='; object-src 'self'",
  "permissions": ["identity","webNavigation", "*://*/*"]
}